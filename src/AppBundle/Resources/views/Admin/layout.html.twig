{% extends '::base.html.twig' %}

{% block stylesheets %}
    {% stylesheets
        'bower_components/metisMenu/dist/metisMenu.min.css'
        'bower_components/startbootstrap-sb-admin-2/dist/css/sb-admin-2.css'
        'bower_components/angular-chosen-localytics/chosen-spinner.css'
        'bower_components/angular-bootstrap/ui-bootstrap-csp.css'
        'bower_components/angular-spinkit/build/angular-spinkit.min.css'
        'bundles/app/css/admin.css'
    %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}
{% block body %}
<div id="wrapper" ng-app="eveTool">
    <!-- Navigation -->
    {% include '::common/navigation.html.twig' %}

    <!-- Page Content -->
    <div id="page-wrapper" >
        {#
        <div id="page-overlay"></div>
        #}
        <div class="container-fluid" >
            {%  include ':common:flashes.html.twig' %}
            {% block content %}
                {% block fos_user_content %}
                {% endblock fos_user_content %}
            {% endblock %}
            <!-- /.row -->
            {% include '::common/footer.html.twig' %}
        </div>
        <!-- /.container-fluid -->
    </div>
    <!-- /#page-wrapper -->

</div>
{% endblock %}
{% block extra_javascript %}
    {{ parent() }}

    {% if app.getUser() %}
    <script>
        var logged_in_user_roles = {{ app.getUser().getRoles() | json_encode | raw }};
        var current_user = {{ app.getUser().getId() | json_encode | raw }};
    </script>
    {% endif %}

    {% javascripts
        'bower_components/humanize-duration/humanize-duration.js'
        'bower_components/angularjs/angular.js'
        'bower_components/AngularSlideables/angularSlideables.js'
